#!/usr/bin/env sh
#
# Ping Identity DevOps - Docker Build Hooks
#
${VERBOSE} && set -x

FILES="configuration.yml keystore.p12"
for FILE in `${FILES}`
do
  SRC_DIR=/opt/staging
  SRC_FILE=${SRC_DIR}/${FILE}
  DEST_DIR=${SERVER_ROOT_DIR}/config
  DEST_FILE=${DEST_DIR}/${FILE}

  echo "Moving ${SRC_FILE} to ${DEST_FILE}"
  cp ${SRC_FILE} ${DEST_FILE}
#cat ${DEST_FILE}
done
